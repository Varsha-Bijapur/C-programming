// to add an element in the starting of the array

# include <stdio.h>

int main(){
    int n,i,j;
    n = 4;
    int newv = 34;
    int arr[] = {1,2,3,4};
    
    for (i=n-1; i>=0; i--){
        arr[i+1] = arr[i];
    }
    arr[0]= newv;
    
    for (i=0; i<n+1; i++){
        printf("%d ", arr[i]);
    }
    printf("\n");
    return 0;
}

//to add the element in the end of the array

# include <stdio.h>

int main(){
    int n,i,j;
    n = 4;
    int newv = 34;
    int arr[] = {1,2,3,4};
    
    arr[n]= newv;
    
    for (i=0; i<n+1; i++){
        printf("%d ", arr[i]);
    }
    printf("\n");
    return 0;
}

